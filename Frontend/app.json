{
  "expo": {
    "name": "my-expo-app",
    "slug": "my-expo-app",
    "version": "1.0.0",

    "scheme": "my-expo-app",

    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },

    "experiments": {
      "tsconfigPaths": true
    },

    "plugins": [
      [
        "expo-font"
      ]
    ],

    "orientation": "portrait",
    "icon": "./assets/images/icon.png",

    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
